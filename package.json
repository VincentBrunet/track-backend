{
  "name": "finn-backend",
  "version": "1.0.0",
  "description": "",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/fuzzy-search": "^2.1.0",
    "@types/knex": "^0.16.1",
    "@types/moment": "^2.13.0",
    "@types/node": "^13.13.4",
    "@types/pg": "^7.14.3",
    "@types/sqlite3": "^3.1.6",
    "@types/typescript": "^2.0.0",
    "@types/yargs": "^15.0.5",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@types/source-map-support": "^0.5.3",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fuzzy-search": "^3.2.1",
    "knex": "^0.21.1",
    "moment": "^2.25.1",
    "pg": "^8.0.3",
    "source-map-support": "^0.5.19",
    "sqlite3": "^4.2.0",
    "ts-node": "^8.10.1",
    "yargs": "^15.4.1"
  },
  "scripts": {
    "dev-compile": "tsc -w",
    "dev-run-reader": "nodemon -w dist dist/src/index.js --app=reader",
    "dev-run-writer": "nodemon -w dist dist/src/index.js --app=writer",
    "dev-run": "nodemon -w dist dist/src/index.js --app=all",
    "dev-reader": "npm-run-all --parallel dev-compile dev-run-reader",
    "dev-writer": "npm-run-all --parallel dev-compile dev-run-writer",
    "dev": "npm-run-all --parallel dev-compile dev-run",
    "knex:migrate:make": "knex --knexfile ./src/config/knexfile.ts -x ts migrate:make",
    "knex:migrate:latest": "knex --knexfile ./src/config/knexfile.ts migrate:latest",
    "knex:migrate:rollback": "knex --knexfile ./src/config/knexfile.ts migrate:rollback",
    "knex:migrate:purge": "npm-run-all knex:migrate:rollback knex:migrate:latest"
  }
}
